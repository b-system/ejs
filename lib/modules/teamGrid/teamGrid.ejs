<%

    const props = {
        teamGrid: teamGrid,
    }
%>

    <section class="section section-divider team-grid d-grid p-0">
        <% props.teamGrid.forEach((user)=> { %>
            <div class="team-grid-<%- user.onGrid -%>">
                <%- include(`${component}/gridCell/basic`, {
                    name: user.name,
                    job: user.job,
                    img: {
                        src: user.img.src, 
                        alt: user.img.alt
                    },
                    social: ['twitter', 'behance', 'linkedin'],
                    link: {
                        href: user.link.href,
                        label: user.link.label,
                        icon: user.link.icon,
                    },
                    shape: user.shape
                }) %>
            </div>
        <% }) %>
    </section>
<%
    const props = {
        href: typeof href !== 'undefined' ? href : '#',
        label: typeof label !== 'undefined' ? label : null,
        icon: typeof icon !== 'undefined' ? icon : null,
        iconPosition: typeof iconPosition !== 'undefined' ? iconPosition : 'before',
    }
%>

<%- include( `${component}/links/link`, {
    href: props.href,
    label: props.label,
    icon: props.icon,
    iconPosition: props.iconPosition
} ) %>
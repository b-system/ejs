<%
    /**
    * @required {array} items
    */
    const props = {
        items: items,
    }

    function validate( props ) {
        const keys = props.items.map( ( item ) => Object.keys( item ).filter( ( key ) => ! [ 'label', 'href' ].includes( key ) ) ).flat()
        if ( 0 < keys.length ) {
            throw new Error( `The "${keys.join( ', ' )}" key/s is/are invalid` )
        }
    }
    validate( props )
%>

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <% props.items.forEach( ( item ) => { %>
            <%- include( `${component}/interaction/breadcrumb-item`, { ...item } ) %>
        <% } ) %>
    </ol>
</nav>

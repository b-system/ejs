<%
/**
 * @requires {string} text
 *
 */
const props = {
    text: text
}
function validate(props) {
    if ('string' !== typeof props.text) {
        throw new Error('Text must be a string')
    }
}
validate(props)
%>
<h3 class="space-bottom-48 space-bottom-lg-56 space-bottom-xl-88"><%- props.text -%></h3>

<%- include(`${view}/quick_start/layout`) %>
<%- include(`${view}/quick_start/darkened-layout`) %>

<%- contentFor('body') %>

<% ['light', 'dark'].forEach((section) => { %>
    <!-- Section <%- section -%> -->
    <section class="section section-divider darkened px-0 pb-0"
    <% if(section === 'dark') { %>
             data-b-system-theme="dark"
            <% } %>
    >

        <!--Basic-->
        <div class="container">
            <%- include(`${component}/text/head-x3`, { text: 'Header basic' }) %>
        </div>
        <%
            const basic = {
                title: 'Welcome to B_system',
                isPlus: false,
                hasBorder: false,
                id: 'random-id'
            }

            const basicModule = include(`${module}/basicContent/basic`, basic)

            const codePreview = {
                idAccordion: 'basic',
                idItem: 'basic-item',
                isDarkened: true,
                content: basicModule
            }
        %>
        <div class="section section-divider">
            <%- basicModule %>
        </div>

        <%- include(`${component}/codePreview/codePreview`, codePreview) %>


        <!--END:Basic-->

        <!--Basic Plus-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header basic plus' }) %>
        </div>

        <%
            const basicPlus = {
                title:"Hi, I'm Matias Eriksson, photographer and designer",
                isPlus:true,
                hasBorder: false,
                id: 'random-id-plus'
            }

            const basicPlusModule = include(`${module}/basicContent/basic`, basicPlus)

            const codePreviewPlus = {
                idAccordion: 'basic',
                idItem: 'basic-item-plus',
                isDarkened: true,
                content: basicPlusModule
            }
        %>
        <div class="section section-divider">
            <%- basicPlusModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewPlus) %>

        <!--END:Basic plus-->

        <!--Simple-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header simple ' }) %>
        </div>
        <%
            const basicSimple = {
                variant:'center',
                title:'To get started',
                text:'B_system is a multipurpose template created with the proposal of maximizing efficiency in the construction of digital touch points for any type of brand. It is made with completely scalable and reusable components so you only have to worry about creating the personality of your brand. Create your digital brand in a natural and fluid way.',
                hasBorder: false
            }

            const basicSimpleModule = include(`${module}/basicContent/simple`, basicSimple)

            const codePreviewSimple = {
                idAccordion: 'basic-simple',
                idItem: 'basic-item-simple',
                isDarkened: true,
                content: basicSimpleModule
            }
        %>
        <div class="section section-divider">
            <%- basicSimpleModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewSimple) %>

        <!--END:Simple-->

        <!--Simple 2 columns-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header simple 2 columns', hasBorder: false }) %>
        </div>
        <%
            const basicSimple2 = {
                variant:'2-columns',
                title:'To get started',
                text:'B_system is a multipurpose HTML5 template that will help you create responsive web pages with very little effort. It is made with completely scalable and reusable components, so you only have to worry about creating your wonderful project.',
                hasBorder: false
            }

            const basicSimple2Module = include(`${module}/basicContent/simple`, basicSimple2)

            const codePreviewSimple2 = {
                idAccordion: 'basic-simple',
                idItem: 'basic-item-simple-2col',
                isDarkened: true,
                content: basicSimple2Module
            }
        %>
        <div class="section section-divider">
            <%- basicSimple2Module %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewSimple2) %>

        <!--END:Simple 2 columns-->

        <!--Simple left-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header simple left aligned' }) %>
        </div>
        <%
            const basicSimpleLeft = {
                title:'Visit our gallery',
                text:'B_system is a multipurpose template created with the proposal of maximizing efficiency in the construction of digital touch points for any type of brand. It is made with completely scalable and reusable components so you only have to worry about creating the personality of your brand.',
                hasBorder: false
            }

            const basicSimpleLeftModule = include(`${module}/basicContent/simple`, basicSimpleLeft)

            const codePreviewSimpleLeft = {
                idAccordion: 'basic-simple',
                idItem: 'basic-item-simple-left',
                isDarkened: true,
                content: basicSimpleLeftModule
            }
        %>
        <div class="section section-divider">
            <%- basicSimpleLeftModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewSimpleLeft) %>

        <!--END:Simple left-->

        <!--Simple left fit-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header simple left aligned fit' }) %>
        </div>
        <%
            const basicSimpleLeftFit = {
                variant:'start-fit',
                title:'Visit our gallery',
                text:'B_system is a multipurpose template created with the proposal of maximizing efficiency in the construction of digital touch points for any type of brand. It is made with completely scalable and reusable components so you only have to worry about creating the personality of your brand.',
                hasBorder: false
            }

            const basicSimpleLeftFitModule = include(`${module}/basicContent/simple`, basicSimpleLeftFit)

            const codePreviewSimpleLeftFit = {
                idAccordion: 'basic-simple',
                idItem: 'basic-item-simple-fit',
                isDarkened: true,
                content: basicSimpleLeftFitModule
            }
        %>
        <div class="section section-divider">
            <%- basicSimpleLeftFitModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewSimpleLeftFit) %>

        <!--END:Simple left fit-->

        <!--Argument-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Arguments module' }) %>
        </div>
        <%
            const basicArgument = {
                columns: [
                    {
                        alignCenter: false,
                        title: 'Agility',
                        text: 'Companies that dominate competitive agility achieve their growth, profitability, sustainability and trust objectives.'
                    },
                    {
                        alignCenter: false,
                        title: 'Innovation',
                        text: 'Companies must manage their business in order to maximize their investments in innovation and restructure their resources.'
                    },
                    {
                        alignCenter: false,
                        title: 'Disruption',
                        text: 'Is a competition demanded from the outside and driven from the inside. Leaders turn disruption into an opportunity.'
                    }
                ],
                isPlus: true,
                alignCenter: false,
                hasBorder: false
            }

            const basicArgumentModule = include(`${module}/basicContent/argument`, basicArgument)

            const codePreviewArgument = {
                idAccordion: 'basic-argument',
                idItem: 'basic-item-argument',
                isDarkened: true,
                content: basicArgumentModule
            }
        %>
        <div class="section section-divider">
            <%- basicArgumentModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewArgument) %>

        <!--END:Argument-->

        <!--Description module-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Description module' }) %>
        </div>
        <%
            const basicDescription = {
                title:'Create your digital brand in an agile and fluid way',
                text: 'B_system is a multipurpose template created with the proposal of maximizing efficiency in the construction of digital touch points for any type of brand. It is made with completely scalable and reusable components so you only have to worry about creating the personality of your brand.',
                hasBorder: false
            }

            const basicDescriptionModule = include(`${module}/basicContent/description`, basicDescription)

            const codePreviewDescription = {
                idAccordion: 'basic-description',
                idItem: 'basic-item-description',
                isDarkened: true,
                content: basicDescriptionModule
            }
        %>
        <div class="section section-divider">
            <%- basicDescriptionModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewDescription) %>

        <!--END:Description module-->

        <!--Header 2 columns-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header 2 columns' }) %>
        </div>
        <%
            const basic2Columns = {
                    title: 'Meet the team that has won so many awards',
                    text: 'Example of how to make your team members known in a creative way. You have a multitude of design combinations.',
                    icon: 'awards-circle',
                    link: { label:'Get to know our awards',
                    href:'#'
                },
                hasBorder: false
            }

            const basic2ColumnsModule = include(`${module}/basicContent/half-content`, basic2Columns)

            const codePreview2Columns = {
                idAccordion: 'basic-columns',
                idItem: 'basic-item-columns',
                isDarkened: true,
                content: basic2ColumnsModule
            }
        %>
        <div class="section section-divider">
            <%- basic2ColumnsModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreview2Columns) %>

        <!--END:Header 2 columns-->

        <!--Header image-->
        <div class="container space-top-48 space-top-lg-56 space-top-xl-88">
            <%- include(`${component}/text/head-x3`, { text: 'Header image' }) %>
        </div>
        <%
            const basicImage = {
                img: {
                    src: 'basic-content-01.png',
                    alt: 'Header image'
                },
                text: 'B_system is a multipurpose HTML5 template that will help you build responsive web pages with very little effort. It is made with fully scalable and reusable components, so you only have to worry about creating your wonderful project.',
                link: {
                    href:'#',
                    label:'Learn more'
                },
                hasBorder: false
            }

            const basicImageModule = include(`${module}/basicContent/image`, basicImage)

            const codePreviewImage = {
                idAccordion: 'basic-image',
                idItem: 'basic-item-image',
                isDarkened: true,
                content: basicImageModule
            }
        %>
        <div class="section section-divider">
            <%- basicImageModule %>
        </div>
        <%- include(`${component}/codePreview/codePreview`, codePreviewImage) %>

        <!--END:Header image-->

    </section>
    <!-- END:Section <%- section -%> -->
<% }) %>

<%- include(`${view}/quick_start/layout`) %>

<%- contentFor('body') %>

<section class="section darkener">
    <div class="container">
        <h3 class="inner-space-block-48 inner-space-block-lg-88">Scenes</h3>
        <div class="d-grid grid-2">
            <div class="space-y-24">
                <div class="demo-card --scene">
                    <%- include(`${component}/illustrations/illustration`, {
                    type: 'scene',
                    illustration: 'couple',
                    } ) %>
                </div>
                <span class="body-light-gray-text">
                    Couple
                </span>
            </div>
            <div class="space-y-24">
                <div class="demo-card --scene">
                    <%- include(`${component}/illustrations/illustration`, {
                    type: 'scene',
                    illustration: 'couple',
                    } ) %>
                </div>
                <span class="body-light-gray-text">
                    Couple
                </span>
            </div>
        </div>
    </div>

</section>


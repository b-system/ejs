<%- include(`${view}/quick_start/layout`) %>

<%- contentFor('body') %>
<!--PRODUCT CARDS-->

<!--SECTION LIGHT-->
<section class="section">

    <div class="container">

        <h3 class="space-bottom-48 space-bottom-md-56 space-bottom-lg-88">Product cards</h3>

        <div class="d-grid grid-4 grid-sm-8 grid-xl-12 column-gap-24 row-gap-48">
            
            <div class="col-1-to-5 d-flex flex-column space-y-24">
                <%- include(`${component}/productCards/basic`, { 
                    img: 'light_product-basic.jpg', 
                    imgAlt: 'Product image',
                    name:'T-shirt Perkins',
                    icon1: 'heart',
                    icon2: 'shopping',
                    price: '$ 12.99',
                    hrefCard: '#',
                    labelLink: 'View more',
                    iconLink: 'links',
                    colors: ['#000000', '#b8ddff', '#f6f6f6'],
                }) %>
                <span class="body-light-gray-text">
                    Product basic
                </span>
            </div>

            <div class="col-1-to-5 col-sm-5-to-9 space-y-24">
                <%- include(`${component}/productCards/product`, { 
                    img: 'light_product-card.jpg', 
                    imgAlt: 'Product image',
                    name:'T-shirt Perkins ',
                    icon1: 'heart',
                    icon2: 'shopping',
                    price: '$ 12.99',
                    hrefCard: '#',
                    labelLink: 'View more',
                    iconLink: 'links',
                }) %>
